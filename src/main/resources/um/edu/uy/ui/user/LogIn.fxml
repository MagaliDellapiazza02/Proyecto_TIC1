    <children>
        <StackPane>
            <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
                <children>
               <Pane layoutY="181.0" prefHeight="149.0" prefWidth="677.0">
                  <children>
                          <Button fx:id="btnLogin" defaultButton="true" layoutX="295.0" layoutY="122.0" mnemonicParsing="false" onAction="#logInButtonClicked" text="Iniciar sesión" textFill="WHITE" />
                          <TextField fx:id="txtUsername" layoutX="255.0" promptText="Mail" />
                          <PasswordField fx:id="txtPassword" layoutX="255.0" layoutY="56.0" promptText="Contraseña" />
                          <ImageView fitHeight="26.0" fitWidth="24.0" layoutX="222.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                  <Image url="@/um/edu/uy/ui/user/stylebases/user.png" />
                              </image>
                          </ImageView>
                          <ImageView fitHeight="26.0" fitWidth="24.0" layoutX="222.0" layoutY="56.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                  <Image url="@/um/edu/uy/ui/user/stylebases/password.png" />
                              </image>
                          </ImageView>
                  </children>
               </Pane>
                    <fx:define>
                        <!-- algo -->
                    </fx:define>
                    <Button fx:id="btnSignUp" layoutX="533.0" layoutY="354.0" mnemonicParsing="false" onAction="#registrarmeButtonClicked" text="Registrarme" />
                    <ImageView fitHeight="66.0" fitWidth="677.0" layoutX="159.0" layoutY="75.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@/um/edu/uy/ui/user/stylebases/easeflight_logo.png" />
                        </image>
                    </ImageView>
                </children>
            </AnchorPane>
        </StackPane>
    </children>
</VBox>
